{
  "name": "Minecraft-DT2",
  "version": "0.1.3",
  "description": "Minecraft-DT is a simple launcher for special development tools which are a third party editors based on game known as Minecraft. Those editors save users time by completing complex instructions in the background.",
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "ini": ">=1.3.6"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "build-installer": "electron-builder",
    "build": "npm run react-build && npm run build-installer",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron . \""
  },
  "build": {
    "appId": "Minecraft-DT2",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icon.ico"
    },
    "nsis": {
      "installerIcon": "src/icon.ico",
      "uninstallerIcon": "src/icon.ico",
      "uninstallDisplayName": "Minecraft-DT2",
      "license": "LICENSE",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "homepage": "./",
  "keywords": [],
  "author": {
    "name": "McRaZick",
    "email": "mcrazick@mail.com"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.2.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0",
    "ini": ">=1.3.6"
  }
}
